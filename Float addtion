/*
  Q.1 : Add two float values
    input :
    a = 4.1
    b = 2.1
    
    output:
    result = 6.20
    
    Note : upto max 2 decimal values
    


*/
#include <stdio.h>


int main()
{
    float x,y,c;
	printf("ENTER Two NUMBERS\n");
	scanf("%f %f",&x,&y);
    
    c=x+y;
    printf("RESULt=%.2f",c);
    
return 0;
}
